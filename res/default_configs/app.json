{
    "log.level": "DEBUG",
    "log.num_of_files": 5,
    "log.format": "[%(asctime)s.%(msecs)03d][%(levelname)s][%(name)s.%(funcName)s]: %(message)s",
    "log.date_format": "%d.%m.%Y %H:%M:%S",
    "log.file_name": "%d-%m-%Y-%H-%M-%S.log",
    "language": "System",
    "ui.mode": "System",
    "use_hardlinks": true,
    "replace_when_merge": true
}